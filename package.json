{
    "name": "create-file-folder",
    "displayName": "Create File & Folder",
    "description": "Create Files & Folder in Atom Style",
    "version": "0.0.1",
    "publisher": "ritwickdey",
    "engines": {
        "vscode": "^1.14.0"
    },
    "author": {
        "name": "Ritwick Dey",
        "email": "ritwickdey@outlook.com",
        "url": "ritwickdey.github.io"
    },
    "keywords": [
        "create files",
        "create folders"
    ],
    "categories": [
        "Other"
    ],
    "preview": true,
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createFile",
                "title": "Create File"
            },
            {
                "command": "extension.createFolder",
                "title": "Create Folder"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}